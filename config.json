            },
            {
              "id": "UBUNTU",
              "name": "Ubuntu with Docker"
            },
            {
              "id": "UBUNTU_CONTAINERD",
              "name": "Ubuntu with Containerd"
            },
            {
              "id": "WINDOWS_LTSC",
