{
  "name": "Google-Cloud-Kubernetes-Engine",
  "type": "executer",
  "exposeRoute": false,
  "execProgram": "node",
  "main": "app.js",
  "imgUrl": "logo.png",
  "version": "1.0.5",
  "description": "Kaholo plugin for integration with Google Cloud Kubernetes Engine(GKE).",
  "category": ["CLOUD", "CONTAINERS"],
  "keywords": ["GCP", "kubernetes", "k8", "gke", "google", "run", "google", "platform", "vm", "instance", "cloud", "cluster", "project", "infra", "infrastructure", "cli", "network", "subnetwork", "firewall", "IP", "route", "storage"],
  "settings": [
    {
      "name": "creds",
      "viewName": "Default Service Account Credentials",
      "type": "vault",
      "description": "Default service account credentials",
      "learnUrl": "https://cloud.google.com/docs/authentication/production"
    },
    {
      "name": "project",
      "viewName": "Default Project ID",
      "type": "string",
      "description": "The ID of the default project to send requests to.",
      "placeholder": "my-project",
      "learnUrl": "https://cloud.google.com/resource-manager/docs/creating-managing-projects"
    },
    {
      "name": "region",
      "viewName": "Default Region",
      "type": "string",
      "description": "Geographical GCP region where infrastructure is or will be located",
      "placeholder": "us-east1",
      "learnUrl": "https://cloud.google.com/compute/docs/regions-zones"
    }
  ],
  "methods": [
    {
      "name": "createBasicCluster",
      "viewName": "Create Basic Cluster",
      "params": [
        {
          "name": "creds",
          "viewName": "Service Account Credentials *",
          "required": false,
          "type": "vault",
          "description": "Service account credentials.",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "project",
          "viewName": "Project *",
          "required": false,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listProjectsAuto",
          "description": "Project name.",
          "placeholder": "project-name-username",
          "learnUrl": "https://cloud.google.com/resource-manager/docs/creating-managing-projects"
        },
        {
          "name": "name",
          "viewName": "Name *",
          "required": false,
          "type": "string",
          "description": "Name of the new cluster.",
          "placeholder": "clutserName",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/how-to/creating-a-zonal-cluster"
        },
        {
          "name": "locationType",
          "viewName": "Location Type",
          "required": false,
          "type": "options",
          "description": "Whether to create a Regional or a Zonal (single-zone) cluster, Default is Zonal",
          "placeholder": "select using drop-down",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/how-to/creating-a-zonal-cluster",
          "options": [
            {
              "id": "Zonal",
              "name": "Zonal"
            },
            {
              "id": "Regional",
              "name": "Regional"
            }
          ]
        },
        {
          "name": "region",
          "viewName": "Region",
          "required": false,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listRegionsAuto",
          "description": "Geographical GCP region where the new cluster will be located, required if Location Type is Regional.",
          "placeholder": "us-east1",
          "learnUrl": "https://cloud.google.com/compute/docs/regions-zones"
        },
        {
          "name": "zone",
          "viewName": "Zone",
          "required": false,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listZonesAuto",
          "description": "Zone is a subdivision of GCP regions, required if Location Type is Zonal.",
          "placeholder": "us-east1-c",
          "learnUrl": "https://cloud.google.com/compute/docs/regions-zones"
        },
        {
          "name": "controlPlaneReleaseChannel",
          "viewName": "Control Plane Release Channel",
          "required": false,
          "type": "options",
          "description": "Release channels offer the ability to balance between stability and rapid adoption of new releases of Kubernetes. Default is Regular Channel.",
          "placeholder": "select using drop-down",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/release-channels",
          "options": [
            {
              "id": "none",
              "name": "None - Static"
            },
            {
              "id": "RAPID",
              "name": "Rapid Channel"
            },
            {
              "id": "REGULAR",
              "name": "Regular Channel"
            },
            {
              "id": "STABLE",
              "name": "Stable Channel"
            }
          ]
        },
        {
          "name": "version",
          "viewName": "Cluster Version *",
          "required": false,
          "type": "string",
          "description": "Initial cluster version.",
          "placeholder": "1.21.6-gke.1500",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/release-notes"
        },
        {
          "name": "numberOfNodes",
          "viewName": "Number Of Nodes *",
          "required": false,
          "type": "string",
          "description": "Null.",
          "placeholder": "3",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/node-pools"
        },
        {
          "name": "enableAutoscaling",
          "viewName": "Enable Autoscaling",
          "required": false,
          "type": "boolean",
          "description": "Automatically resize the number of nodes in a given node pool, based on the demands of your workloads.",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/cluster-autoscaler",
          "placeholder": "Disable / Enable"
        },
        {
          "name": "minNode",
          "viewName": "Minimum Number Of Nodes",
          "required": false,
          "type": "string",
          "placeholder": "0",
          "description": "The number of nodes in the given node pool to partake in the workload.",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/cluster-autoscaler"
        },
        {
          "name": "maxNode",
          "viewName": "Maximum Number Of Nodes",
          "required": false,
          "type": "string",
          "placeholder": "3",
          "description": "For Autoscaling. Maximum number of nodes in the cluster's node pool.",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/cluster-autoscaler"
        },
        {
          "name": "machineType",
          "viewName": "Machine Type *",
          "required": false,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listMachineTypesAuto",
          "description": "Every machine family has predefined machine shapes that have a specific vCPU to memory ratio that fits a variety of workload needs. If a predefined machine type does not meet your needs, you can create a custom machine for any general-purpose VM.",
          "placeholder": "Custom N1",
          "learnUrl": "https://cloud.google.com/compute/docs/machine-types"
        },
        {
          "name": "nodeImage",
          "viewName": "Node Image Type",
          "required": false,
          "type": "options",
          "description": "The type of container image to run on these nodes, Default is COS with Containerd",
          "placeholder": "select using drop-down",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/concepts/node-images",
          "options": [
            {
              "id": "COS_CONTAINERD",
              "name": "Container-Optimized OS with Containerd"
            },
            {
              "id": "COS",
              "name": "Container-Optimized OS with Docker"
            },
            {
              "id": "UBUNTU",
              "name": "Ubuntu with Docker"
            },
            {
              "id": "UBUNTU_CONTAINERD",
              "name": "Ubuntu with Containerd"
            },
            {
              "id": "WINDOWS_LTSC",
              "name": "Windows Long Term Servicing Channel with Docker"
            },
            {
              "id": "WINDOWS_LTSC_CONTAINERD",
              "name": "Windows Long Term Servicing Channel with Containerd"
            },
            {
              "id": "WINDOWS_SAC",
              "name": "Windows Semi-Annual Channel with Docker"
            },
            {
              "id": "WINDOWS_SAC_CONTAINERD",
              "name": "Windows Semi-Annual Channel with Containerd"
            }
          ]
        },
        {
          "name": "diskType",
          "viewName": "Boot Disk Type, default is PD-Standard",
          "required": false,
          "type": "options",
          "description": "Null.",
          "placeholder": "select using drop-down",
          "learnUrl": "https://cloud.google.com/compute/docs/disks",
          "options": [
            {
              "id": "pd-standard",
              "name": "PD-Standard"
            },
            {
              "id": "pd-balanced",
              "name": "PD-Balanced"
            },
            {
              "id": "pd-ssd",
              "name": "PD-SSD"
            }
          ]
        },
        {
          "name": "diskSize",
          "viewName": "Boot Disk Size(In GB)",
          "required": false,
          "type": "string",
          "description": "The size in GB for the boot disk of each node in the pool",
          "placeholder": "10",
          "learnUrl": "https://cloud.google.com/compute/docs/disks"
        },
        {
          "name": "diskEncryptionKey",
          "viewName": "Boot Disk Encryption Key",
          "required": false,
          "type": "string",
          "description": "Boot Disk Encryption- The process of encrypting all data on the bootable media of a. system, the media responsible for starting the operating system. Ciphertext - Information that has been encrypted, making it unreadable without knowledge.",
          "placeholder": "my-key",
          "learnUrl": "https://cloud.google.com/kubernetes-engine/docs/how-to/using-cmek"
        },
        {
          "name": "waitForOperation",
          "viewName": "Wait For Operation End",
          "required": false,
          "type": "boolean",
          "description": "Waiting until the operation is completed before moving on to the next operation.",
          "learnUrl": "https://github.com/Kaholo/kaholo-plugin-google-cloud-kubernetes-engine"
        }
